<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/videoItemContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null">

    <com.shuashuakan.android.modules.widget.customview.RecommendVideoViewPage
        android:id="@+id/special_view_page_root"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.shuashuakan.android.modules.widget.customview.ViewPlayBottomlProgressBar
        android:id="@+id/video_progress"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="2dp" />
</FrameLayout>
